<div class="breadcrumb_section bg_gray page-title-mini pb-3 pt-3">
    <div class="container">
        <div class="row align-items-center">
            <div class="col-md-6">
                <div class="page-title">
                    <h1>{{ title }}</h1>
                </div>
            </div>
            <div class="col-md-6">
                <ol class="breadcrumb justify-content-md-end">
                    {% if breadcrumbItems is defined %}
                        {% for item in breadcrumbItems %}
                            <li class="breadcrumb-item" {{ item.activeLink ? 'active' : '' }}>
                                <a href="{{ item.activeLink ? '#' : path(item.link) }}"
                                        {{ item.activeLink == app.request.get('_route') ? 'disabled' : '' }}>
                                    {{ item.name }}</a></li>
                        {% endfor %}
                    {% endif %}
                </ol>
            </div>
        </div>
    </div>
</div>